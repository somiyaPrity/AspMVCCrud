@model List<AspMVCCrud.Models.Domain.Employee>


<table class="table">
    <thead>
        <tr>
            <td>ID</td>
            <td>Name</td>
            <td>Email</td>
            <td>DOB</td>
            <td>Department</td>
            <td>Salary</td>
            <td></td>
        </tr>
    </thead>
    <tbody>
        
        @foreach(var employee in Model)
        {
            <tr>
                <td>@employee.id</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.Dob</td>
                <td>@employee.Department</td>
                <td>@employee.Salary</td>
                <td>View</td>
               
            </tr>
        }
    </tbody>
</table>